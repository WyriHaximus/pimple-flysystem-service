{
    "name": "wyrihaximus/pimple-flysystem-service",
    "description": "Flysystem Service for Pimple",
    "license": "MIT",
    "keywords": [
        "flysystem",
        "filesystem",
        "pimple",
        "service"
    ],
    "authors": [
        {
            "name": "Cees-Jan Kiewiet",
            "email": "ceesjank@gmail.com",
            "homepage": "http://wyrihaximus.net/"
        }
    ],
    "require": {
        "php": "^5.4||^7.0",
        "league/flysystem": "^1.0",
        "pimple/pimple": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "WyriHaximus\\Pimple\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WyriHaximus\\Pimple\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phake/phake": "^1.0",
        "phpunit/phpunit": "^5.0||^4.8",
        "squizlabs/php_codesniffer": "~2.8",
        "vectorface/dunit": "~2.0"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "5.4"
        }
    }
}
